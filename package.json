{
  "name": "tarkov",
  "version": "1.0.2",
  "description": "A client library for Escape from Tarkov API",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf ./lib && tsc || del -rf \"./lib\" && tsc",
    "docs": "rm -rf ./docs && typedoc --out docs src || del -rf \"./docs\" && typedoc --out docs src",
    "docs:serve": "npm run docs && npx http-server docs",
    "dev": "tsnd --respawn testing.ts"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matias-kovero/tarkov.git"
  },
  "keywords": [
    "eft",
    "api",
    "sdk",
    "api",
    "escapefromtarkov",
    "tarkov",
    "bot",
    "escape-from-tarkov"
  ],
  "author": "mkovero",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matias-kovero/tarkov/issues"
  },
  "homepage": "https://github.com/matias-kovero/tarkov#readme",
  "dependencies": {
    "got": "10.7.0",
    "pako": "1.0.11",
    "reflect-metadata": "0.1.13",
    "tsyringe": "4.1.0"
  },
  "devDependencies": {
    "@types/got": "9.6.9",
    "@types/pako": "1.0.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typedoc": "0.17.4",
    "typedoc-plugin-external-module-name": "3.0.0",
    "typescript": "3.8.3"
  }
}
