{
  "name": "tarkov",
  "version": "1.0.2",
  "description": "A client library for Escape from Tarkov API",
  "keywords": [
    "eft",
    "api",
    "sdk",
    "escapefromtarkov",
    "tarkov",
    "bot",
    "escape-from-tarkov"
  ],
  "homepage": "https://github.com/matias-kovero/tarkov#readme",
  "bugs": {
    "url": "https://github.com/matias-kovero/tarkov/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matias-kovero/tarkov.git"
  },
  "license": "MIT",
  "author": "mkovero",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc --project tsconfig.json",
    "clean": "rimraf build",
    "dev": "tsnd --respawn testing.ts",
    "docs": "rimraf docs && typedoc --out docs src",
    "docs:serve": "run-s docs && npx http-server docs",
    "lint": "eslint . --ext .ts",
    "lint:fix": "run-s \"lint -- --fix\"",
    "prepublishOnly": "run-s clean build",
    "preversion": "run-s clean lint test build"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "got": "10.7.0",
    "pako": "1.0.11",
    "reflect-metadata": "0.1.13",
    "tsyringe": "4.1.0"
  },
  "devDependencies": {
    "@types/got": "9.6.9",
    "@types/pako": "1.0.1",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.4",
    "rimraf": "3.0.2",
    "ts-node-dev": "1.0.0-pre.44",
    "typedoc": "0.17.4",
    "typedoc-plugin-external-module-name": "3.0.0",
    "typescript": "3.8.3"
  },
  "engines": {
    "node": ">=10"
  }
}
